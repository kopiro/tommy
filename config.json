{
	"version": "v1",
	"remoteSync": false,
	"s3Bucket": null,
	"execDebug": false,
	"processor": {
		"resize": true,
		"image": true,
		"poster": true,
		"jpg": true,
		"png": true,
		"gif": true,
		"svg": true,
		"lazyLoadBlurried": true,
		"videoThumbs": true
	},
	"converter": {
		"mp4": true,
		"webm": true,
		"webp": true,
		"mp3": true,
		"ttf": true,
		"otf": true,
		"svg": true,
		"eot": true,
		"woff": true,
		"woff2": true
	},
	"tester": {
		"font": true,
		"image": true,
		"video": true
	},
	"resize": {
		"suffix": "-resized-${i}.${ext}",
		"dimensions": [200, 400, 800, 1200],
		"quality": 80
	},
	"image": {
		"quality": 80
	},
	"videoThumbs": {
		"suffix": "-thumb-${i}.jpg",
		"size": 400,
		"quality": 80,
		"count": 5
	},
	"poster": {
		"suffix": "-poster.jpg"
	},
	"lazyLoadBlurried": {
		"suffix": "-blurried.jpg",
		"size": 10
	},
	"ignore": [
		".DS_Store",
		".gitignore"
	]
}